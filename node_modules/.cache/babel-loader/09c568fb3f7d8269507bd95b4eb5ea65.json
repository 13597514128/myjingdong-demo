A,KAAK,EAAC,OA4BF;AA1BH,MAAA,GAAG,EAAE;AA0BF,KA7BR,E,CAKI,mBAAA,CAKM,KALN,EAAA,UAAA,EAKM,C,kCAJF,IAAI,CAAC,Q,IAAU,G,EACjB;AAAA;KAGI,EAHJ,mBAAA,CAEO,MAFP,EAAA,UAAA,EAEO,gBAAA,CADD,IAAI,CAAC,UAAL,GAAe,KAAf,GAAe,KACd,CAFP,EACqB;AAAA;AADrB,KAGI,CALN,C,EAMA,mBAAA,CAiBM,KAjBN,EAAA,UAAA,EAiBM,CAhBF,mBAAA,CAWM,KAXN,EAAA,UAAA,EAWM,E,kBAVF,mBAAA,CASW,SATX,EASW,IATX,EASW,WAAA,CARuB,IAAI,CAAC,QAQ5B,EARoC,UAAvC,SAAuC,EAA5B,UAA4B,EAAlB;;aACvB;SAAU,CAKF,UAAU,IAAA,C,kBAHpB,mBAAA,CAIE,KAJF,EAIE;cAAA;AAHE,QAAA,KAAK,EAAC,qBAGR;AAFG,QAAA,GAAG,EAAE,SAAS,CAAC,OAAV,CAAkB;AAE1B,OAJF,E,IAAA,E;;AAAA,Q,UAAA,C,qCAFY,C;;;KAOL,CATX,E;;AAAA,KAUE,EAXN,CAgBE,EAJJ,mBAAA,CAGM,KAHN,EAAA,UAAA,EAGM,CAFF,mBAAA,CAA8D,KAA9D,EAAA,WAAA,EAAmC,OAAE,gBAAA,CAAE,IAAI,CAAC,UAAP,CAArC,EAAsD;AAAA;AAAtD,KAEE,EADF,mBAAA,CAAiE,KAAjE,EAAA,WAAA,EAAmC,OAAE,gBAAA,CAAE,IAAI,CAAC,WAAP,CAAF,GAAsB,IAAzD,EAA2D;AAAA;AAA3D,KACE,CAHN,CAII,CAjBN,C,CAXJ,C;GA6BQ,CA7BR,E;;AAAA,GA8BI,EA/BN,CAgCE,CAlCN,C,EAmCA,YAAA,CAA6B,iBAA7B,EAA6B;AAApB,IAAA,YAAY,EAAI;AAAI,GAA7B,C","sourcesContent":["<template>\r\n  <div class=\"wrapper\">\r\n      <div class=\"title\">我的订单</div>\r\n      <div class=\"orders\">\r\n        <div \r\n            class=\"order\"\r\n            v-for=\"(item, index) in list\"\r\n            :key=\"index\"\r\n        >\r\n            <div class=\"order__title\">\r\n              {{item.shopName}}\r\n              <span class=\"order__status\">\r\n                  {{item.isCanceled ? '已取消' : '已下单'}}\r\n              </span>\r\n            </div>\r\n            <div class=\"order__content\">\r\n                <div class=\"order__content__imgs\">\r\n                    <template\r\n                    v-for=\"(innerItem, innerIndex) in item.products\"\r\n                    :key=\"innerIndex\"\r\n                    >\r\n                        <img \r\n                            class=\"order__content__img\"     \r\n                            :src=\"innerItem.product.img\" \r\n                            v-if=\"innerIndex <= 3\" \r\n                        />\r\n                    </template>\r\n                </div>\r\n              <div class=\"order__content__info\">\r\n                  <div class=\"order__content__price\">¥ {{item.totalPrice}}</div>\r\n                  <div class=\"order__content__count\">共 {{item.totalNamber}} 件</div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n      </div>\r\n  </div>\r\n  <Docker :currentIndex = \"2\"/>\r\n</template>\r\n\r\n<script>\r\nimport { reactive, toRefs } from 'vue'\r\nimport { get } from '../../utils/request'\r\nimport Docker from '../../components/Docker'\r\n//处理订单列表的相关逻辑\r\nconst useOrderListEffect = () => {\r\n    const data = reactive({list:[]})\r\n    const getNearbyList = async () => {\r\n        const result = await get('/api/order');\r\n        if (result?.errno === 0 && result?.data?.length) {\r\n            const orderList = result.data;\r\n            orderList.forEach((order)=>{\r\n                const products = order.products || [];\r\n                let totalPrice = 0;\r\n                let totalNamber = 0;\r\n                products.forEach((productItem) => {\r\n                    totalNamber += ((productItem?.orderSales) || 0);\r\n                    totalPrice += ((productItem?.product?.price * productItem?.orderSales) || 0);\r\n                })\r\n                order.totalPrice = totalPrice;\r\n                order.totalNamber = totalNamber;\r\n            })\r\n            data.list = result.data\r\n        }\r\n    }\r\n    getNearbyList()\r\n    const { list } = toRefs(data)\r\n    return { list }\r\n}\r\nexport default {\r\n    name: 'OrderList',\r\n    components: { Docker },\r\n    setup() {\r\n        const { list } = useOrderListEffect()\r\n        return { list }\r\n    },\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n@import '../../style/viriables.scss';\r\n.wrapper {\r\n  overflow-y: auto;\r\n  position: absolute;\r\n  left: 0;\r\n  top: 0;\r\n  bottom: .5rem;\r\n  right: 0;\r\n  background: rgb(248, 248, 248);\r\n}\r\n.title {\r\n  font-size: .16rem;\r\n  color: $content-fontcolor;\r\n  text-align: center;\r\n  line-height: .44rem;\r\n  background: $bgColor;\r\n}\r\n.order {\r\n  margin: .16rem .18rem;\r\n  padding: .16rem;\r\n  background: $bgColor;\r\n  &__title {\r\n    margin-bottom: .16rem;\r\n    line-height: .22rem;\r\n    font-size: .16rem;\r\n    color: $content-fontcolor;\r\n  }\r\n  &__status {\r\n    float: right;\r\n    font-size: .14rem;\r\n    color: $light-fontColor;\r\n  }\r\n  &__content {\r\n    display: flex;\r\n    &__imgs {\r\n      flex: 1;\r\n    }\r\n    &__img {\r\n      width: .4rem;\r\n      height: .4rem;\r\n      margin-right: .12rem;\r\n    }\r\n    &__info {\r\n      width: .7rem;\r\n    }\r\n    &__price {\r\n      margin-bottom: .04rem;\r\n      line-height: .2rem;\r\n      font-size: .14rem;\r\n      color: $hightlight-fontColor;\r\n      text-align: right;\r\n    }\r\n    &__count {\r\n      line-height: .14rem;\r\n      font-size: .12rem;\r\n      color: $content-fontcolor;\r\n      text-align: right;\r\n    }\r\n  }\r\n}\r\n\r\n</style>"],"sourceRoot":""}]}