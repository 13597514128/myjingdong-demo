,KAvCN,EAAA,UAAA,EAuCM,CAtCF,mBAAA,CAWM,KAXN,EAAA,UAAA,EAWM,CAVF,mBAAA,CASK,IATL,EASK,IATL,EASK,E,kBARD,mBAAA,CAOK,SAPL,EAOK,IAPL,EAOK,WAAA,CALU,MAAA,CAAA,UAKV,EALoB,UAAlB,IAAkB,EAAd;yBAFX,mBAAA,CAOK,IAPL,EAOK;AANJ,MAAA,KAAK,EAAA,eAAA,CAAA;AAAA,0BAAA,IAAA;AAAA,kCAAqD,MAAA,CAAA,UAAA,KAAe,IAAI,CAAC;AAAzE,OAAA,CAMD;AAJJ,MAAA,GAAG,EAAE,IAAI,CAAC,IAIN;AAHJ,MAAA,OAAK,EAAA;AAAA,eAAQ,MAAA,CAAA,cAAA,CAAe,IAAI,CAAC,GAApB,CAAR;AAAA;AAGD,KAPL,E,iBAME,IAAI,CAAC,I,CANP,EAMW;AAAA;AANX,MAMW,UANX,C;GAOK,CAPL,E;;AAAA,GAQC,EATL,CAUE,CAXN,CAsCE,EA1BF,mBAAA,CAyBM,KAzBN,EAAA,UAAA,EAyBM,E,kBAxBF,mBAAA,CAuBM,SAvBN,EAuBM,IAvBN,EAuBM,WAAA,CArBU,MAAA,CAAA,IAqBV,EArBc,UAAZ,IAAY,EAAR;yBAFZ,mBAAA,CAuBM,KAvBN,EAuBM;AAtBL,MAAA,KAAK,EAAC,eAsBD;AApBJ,MAAA,GAAG,EAAE,IAAI,CAAC;AAoBN,KAvBN,E,CAKI,mBAAA,CAIC,KAJD,EAIC;AAHD,MAAA,KAAK,EAAC,oBAGL;AAFA,MAAA,GAAG,EAAE,IAAI,CAAC,MAEV;AADD,MAAA,GAAG,EAAC;AACH,KAJD,E,IAAA,E;;AAAA,M,UAAA,C,EAKA,mBAAA,CAOM,KAPN,EAAA,UAAA,EAOM,CANF,mBAAA,CAAmD,IAAnD,EAAA,UAAA,EAAmD,gBAAA,CAAhB,IAAI,CAAC,IAAW,CAAnD,EAA4C;AAAA;AAA5C,KAME,EALF,mBAAA,CAAqD,GAArD,EAAA,UAAA,EAAgC,OAAE,gBAAA,CAAE,IAAI,CAAC,KAAP,CAAF,GAAgB,GAAhD,EAAiD;AAAA;AAAjD,KAKE,EAJF,mBAAA,CAGI,GAHJ,EAAA,UAAA,EAGI,CAFA,WAEA,E,kCAF2C,IAAI,CAAC,K,IAAO,G,EACvD;AAAA;KACA,EADA,mBAAA,CAA6D,MAA7D,EAAA,WAAA,EAAoC,MAAC,gBAAA,CAAE,IAAI,CAAC,QAAP,CAArC,EAAoD;AAAA;AAApD,KACA,CAHJ,CAIE,CAPN,C,EAQA,W,CAlBJ,C;GAuBM,CAvBN,E;;AAAA,GAwBE,EAzBN,CA0BE,CAvCN,C","sourcesContent":["<template>\r\n    <div class=\"content\">\r\n        <div class=\"category\">\r\n            <ul >\r\n                <li \r\n                :class=\"{'category__item': true, 'category__item--active': currentTab === item.tab}\"\r\n                v-for=\"item in categories\"\r\n                :key=\"item.name\"\r\n                @click=\"() => handleTabClick(item.tab)\"\r\n                >\r\n                {{item.name}}\r\n                </li>\r\n            </ul>\r\n        </div>\r\n        <div class=\"product\">\r\n            <div\r\n             class=\"product__item\"\r\n             v-for=\"item in list\"\r\n             :key=\"item._id\"\r\n            >\r\n                <img \r\n                class=\"product__item__img\"\r\n                :src=\"item.imgUrl\" \r\n                alt=\"\"\r\n                >\r\n                <div class=\"product__item__detail\">\r\n                    <h4 class=\"product__item__title\">{{item.name}}</h4>\r\n                    <p class=\"product__item__sales\">月售{{item.sales}}件</p>\r\n                    <p class=\"product__item__price\">\r\n                        <span class=\"product__item__yen\">￥</span>{{item.price}}\r\n                        <span class=\"product__item__origin\">￥{{item.oldPrice}}</span>\r\n                    </p>\r\n                </div>\r\n                <div class=\"product__number\">\r\n                    <span class=\"product__number__minus\">-</span>\r\n                    0\r\n                    <span class=\"product__number__plus\">+</span>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport { reactive, ref, toRefs, watchEffect } from 'vue'\r\nimport { useRoute } from 'vue-router'\r\nimport { get } from '../../utils/request'\r\n\r\nconst categories=[\r\n{name:'全部商品',tab:'all'},\r\n{name:'秒杀',tab:'seckill'},\r\n{name:'新鲜水果',tab:'fruit'}]\r\n\r\n\r\n//和tab切换相关的逻辑\r\nconst useTabEffect=()=>{\r\n    const currentTab=ref(categories[0].tab)\r\n    const handleTabClick=(tab)=>{\r\n    currentTab.value = tab\r\n}\r\n    return{ currentTab, handleTabClick }\r\n}\r\n\r\n\r\n//列表内容相关的逻辑\r\nconst useCurrentListEffect = (currentTab) => {\r\n    const route = useRoute();\r\n    const shopId = route.params.id;\r\n    const content = reactive({ list:[] });\r\n\r\n    const getContentData = async() => {\r\n        const result = await get(`/api/shop/${shopId}/products`,{tab:currentTab.value})\r\n        if(result?.errno === 0 && result?.data?.length){\r\n            content.list = result.data\r\n        }\r\n}\r\n    watchEffect( () => { getContentData() })\r\n    const { list } = toRefs(content);\r\n    return{ list }\r\n}\r\nexport default {\r\n    name: 'Content',\r\n    setup(){\r\n        const { currentTab, handleTabClick } = useTabEffect();\r\n        const { list } = useCurrentListEffect(currentTab);\r\n        return { currentTab, categories, handleTabClick, list }\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n@import '../../style/viriables.scss';\r\n@import '../../style/mixins.scss';\r\n.content {\r\n    display: flex;\r\n    position: absolute;\r\n    left: 0;\r\n    right: 0;\r\n    top: 1.5rem;\r\n    bottom: .5rem;\r\n    \r\n}\r\n.category {\r\n    height: 100%;\r\n    width: .76rem;\r\n    background: $search-bgcolor;\r\n    overflow-y: scroll;\r\n    ul {\r\n        list-style: none;\r\n        padding: 0;\r\n        margin: 0;    \r\n    }\r\n    &__item {\r\n        line-height:  .4rem;\r\n        text-align: center;\r\n        font-size: .14rem;\r\n        color: $content-fontcolor;\r\n        &--active{\r\n            background: $bgcolor;\r\n        }\r\n    }\r\n}\r\n.product {\r\n    flex: 1;\r\n    overflow-y: scroll;\r\n    &__item {\r\n        position: relative;\r\n        display: flex;\r\n        padding: .12rem 0;\r\n        margin : 0 .16rem;\r\n        border-bottom: .01rem solid $content-bgcolor;\r\n        &__img {\r\n            width: .68rem;\r\n            height: .68rem;\r\n            margin-right:  .16rem;\r\n        }\r\n        &__detail {\r\n            overflow: hidden;\r\n        }\r\n        &__title{\r\n            margin: 0;\r\n            line-height: .2rem;\r\n            font-size: .16rem;\r\n            color: $content-fontcolor;\r\n            @include ellipsis;\r\n        }\r\n        &__sales {\r\n            margin: .06rem 0;\r\n            line-height: .16rem;\r\n            font-size: .12rem;\r\n            color: $content-fontcolor;\r\n        }\r\n        &__price {\r\n            margin: 0 0 0 -.02rem;\r\n            line-height: .2rem;\r\n            font-size: .14rem;\r\n            color: $hightlight-fontcolor;\r\n        }\r\n        &__yen {\r\n            font-size: .14rem;\r\n        }\r\n        &__origin {\r\n            margin-left:  .06rem;\r\n            line-height: .2rem;\r\n            font-size: .12rem;\r\n            color: $light-fontcolor;\r\n            text-decoration: line-through;\r\n        }\r\n        .product__number {\r\n            position: absolute;\r\n            right: 0;\r\n            bottom: .12rem;\r\n            &__minus, &__plus {\r\n                display: inline-block;\r\n                width: .2rem;\r\n                height: .2rem;\r\n                line-height: .19rem;\r\n                border-radius: 50%;\r\n                font-size: .2rem;\r\n                text-align: center;\r\n            }\r\n            &__minus {\r\n                border: .01rem solid $medium-fontcolor;\r\n                color: $medium-fontcolor;\r\n                margin-right: .05rem;\r\n            }\r\n            &__plus {\r\n                background: $btn-bgcolor;\r\n                color: $bgcolor;\r\n                margin-left: .05rem;\r\n            }\r\n        }\r\n    }\r\n\r\n}\r\n</style>"],"sourceRoot":""}]}